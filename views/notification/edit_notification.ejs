

<link rel="stylesheet" href="http://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">	
<link rel="stylesheet" href="/assets/jquery.tagsinput-revisited.css" />

<link href="/tag/jquery.flexdatalist.css" rel="stylesheet" type="text/css">
<link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/selectize.js/0.12.6/css/selectize.bootstrap3.min.css" integrity="sha256-ze/OEYGcFbPRmvCnrSeKbRTtjG4vGLHXgOqsyLFTRjg=" crossorigin="anonymous" />


<form class="form_custom" id="" method="post" action="/update-notification/<%=data._id %>/<%=event_id%>">
  <div class="adiminBodyContent">
    <!-- Start Page Title Header Starts-->
      <div class="headerTitle">
          <h2>Edit Notification</h2>    
          <% if(typeof errors !== 'undefined' && typeof errors.message !== 'undefined') { %>
            <p class = "alert alert-danger ml15 mr15 text-center"><%- errors.message %></p>
         <% } %>  
         <% if(typeof errors !== 'undefined' && typeof errors.msg !== 'undefined') { %>
            <p class = "alert alert-success ml15 mr15 text-center"><%- errors.msg %></p>
         <% } %>
      </div>
        <!-- End Page Title Header Ends-->
      <div class="midContentRightBar addSessionBlock">
      <div class="meddleContentSec agendaContentInfo">
        <div class="eventInfoBox">
            <div class="infoBoxContent">
         
                <div class="boxContentRow">
                    <span class="infoTitle">Session</span>
                    <div class="infoBoxData">
                        <input type="text" class="form-control" placeholder="Enter session" name="session" id="session" value="<%=data.session %>">
                        <% if(typeof errors !== 'undefined' && typeof errors.session !== 'undefined') { %>
                          <p class = "error"><%- errors["session"] %></p>
                      <% } %> 
                    </div>
                </div>

                <div class="boxContentRow">
                  <span class="infoTitle">Datetime</span>
                  <div class="infoBoxData">
                      <input type="datetime-local" class="form-control" placeholder="Enter datetime" name="datetime" id="datetime"  value="<%=data.datetime %>">
                      <% if(typeof errors !== 'undefined' && typeof errors.datetime !== 'undefined') { %>
                        <p class = "error"><%- errors["datetime"] %></p>
                    <% } %> 
                  </div>
              </div>
              
              
            </div>
        </div>
      
      </div>
        <!-- Start Session Description section -->
            <div class="eventInfoBox">
                <div class="infoBoxTitle">
                    <h3>Message</h3>
                </div>
                <div class="infoBoxContent">
                    <div class="boxContentTextArea">
                        <div class="infoBoxData">
                            <textarea class="form-control" rows="7" placeholder="Enter message" name="messages" id="messages" ><%=data.message %></textarea>
                            <% if(typeof errors !== 'undefined' && typeof errors.messages !== 'undefined') { %>
                              <p class = "error"><%- errors["messages"] %></p>
                          <% } %> 
                        </div>
                        
                    </div>
                </div>
            </div>
          <!-- End Session Description section -->
         
    <!-- End Add video section -->
        </div>
      <!-- Submit Button -->
      <div class="adminSubmitBtn">
          <button type="submit" class="btn btn-primary adminPageBtn">Update Notification</button>
      </div>
      <!-- Submit Button -->
  </div>
</form>
<script src="/assets/js/jquery-3.5.1.min.js"></script>  