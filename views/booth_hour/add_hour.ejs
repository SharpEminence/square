

  <form class="form_custom"  action="/postHour/<%=event_id %>" method="post">
  <div class="adiminBodyContent">
    <!-- Start Page Title Header Starts-->
      <div class="headerTitle">
          <h2>Add Hour</h2>    
          <% if(typeof errors !== 'undefined' && typeof errors.message !== 'undefined') { %>
            <p class = "alert alert-danger ml15 mr15 text-center"><%- errors.message %></p>
         <% } %>  
         <% if(typeof errors !== 'undefined' && typeof errors.msg !== 'undefined') { %>
            <p class = "alert alert-success ml15 mr15 text-center"><%- errors.msg %></p>
         <% } %>      
      </div>
        <!-- End Page Title Header Ends-->
      <div class="midContentRightBar">
      <div class="meddleContentSec agendaContentInfo">
        <div class="eventInfoBox">
            <div class="infoBoxContent">
              
                <div class="boxContentRow">
                  <span class="infoTitle">Select Booth</span>
                  <div class="infoBoxData infoSelectBox">
                      <div class="form-group">
                        <select  name="booth" id="booth" class="form-control" dir="rtl">
                          <option value="">Select Booth</option>
                          <% company_data.map((offer,i) => { %>
                            <option  value="<%= offer._id %>"><%= offer.name %></option>
                            <% }) %>
                        </select>
                        <% if(typeof errors !== 'undefined' && typeof errors.booth !== 'undefined') { %>
                          <p class = "error"><%- errors["booth"] %></p>
                      <% } %> 
                      </div>
                  </div>
              </div>

                <div class="boxContentRow">
                    <span class="infoTitle">Booth Date</span>
                    <div class="infoBoxData">
                        <input type="date" class="form-control" name="conference_date" value="<%=data.conference_date %>" id="conference_date" placeholder="Enter Date">
                        <% if(typeof errors !== 'undefined' && typeof errors.conference_date !== 'undefined') { %>
                          <p class = "error"><%- errors["conference_date"] %></p>
                      <% } %> 
                    </div>
                </div>

                <div class="boxContentRow">
                  <span class="infoTitle">Calender title</span>
                  <div class="infoBoxData">
                      <input type="text" class="form-control"  placeholder="Enter Calender title" name="title" value="<%=data.title %>" id="title">
                      <% if(typeof errors !== 'undefined' && typeof errors.title !== 'undefined') { %>
                      <p class = "error"><%- errors["title"] %></p>
                      <% } %> 
                  </div>
                </div>
                
                <div class="boxContentRow">
                  <span class="infoTitle">Booth Time</span>
                  <div class="infoBoxData">
                      <input type="time" class="form-control" name="start_time" value="<%=data.start_time %>" id="start_time" placeholder="Enter Time">
                      <% if(typeof errors !== 'undefined' && typeof errors.start_time !== 'undefined') { %>
                        <p class = "error"><%- errors["start_time"] %></p>
                    <% } %> 
                  </div>
                </div>

                <div class="boxContentRow">
                  <span class="infoTitle">Google Meet</span>
                  <div class="infoBoxData">
                      <input type="text" class="form-control"  placeholder="Enter google meet" name="google_meet" value="<%=data.google_meet %>" id="google_meet">
                      <% if(typeof errors !== 'undefined' && typeof errors.google_meet !== 'undefined') { %>
                      <p class = "error"><%- errors["google_meet"] %></p>
                      <% } %> 
                  </div>
                </div>
                
            </div>
        </div>
        
      </div>
       
      
  </div>
      <!-- Submit Button -->
      <div class="adminSubmitBtn">
          <button type="submit" class="btn btn-primary adminPageBtn">Add Hour</button>
      </div>
      <!-- Submit Button -->
  </div>
</form>
 

<!-- content-wrapper ends -->

    
<script src="/assets/js/jquery-3.5.1.min.js"></script>   
<script>


  $(function() {
  
$("#lounge_type").click(function(){
if($("#lounge_type").val() == "360_space")
{
  $('.space_code').show();
  $("#space_code").prop('disabled', false);
}
else
{
  $('.space_code').hide();
  $("#space_code").prop('disabled', true);

}
});

var temp="<%=data.booth %>"; 
$("#booth").val(temp);
  });
  </script>       