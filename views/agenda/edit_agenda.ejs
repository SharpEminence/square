

<link rel="stylesheet" href="http://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">	
<link rel="stylesheet" href="/assets/jquery.tagsinput-revisited.css" />

<link href="/tag/jquery.flexdatalist.css" rel="stylesheet" type="text/css">
<link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/selectize.js/0.12.6/css/selectize.bootstrap3.min.css" integrity="sha256-ze/OEYGcFbPRmvCnrSeKbRTtjG4vGLHXgOqsyLFTRjg=" crossorigin="anonymous" />
<script src="https://cdn.ckeditor.com/ckeditor5/24.0.0/classic/ckeditor.js"></script>

<form class="form_custom" id="" method="post" action="/update-agenda/<%=data._id %>/<%=event_id%>">
  <div class="adiminBodyContent">
    <!-- Start Page Title Header Starts-->
      <div class="headerTitle">
          <h2>Edit Agenda</h2>    
          <% if(typeof errors !== 'undefined' && typeof errors.message !== 'undefined') { %>
            <p class = "alert alert-danger ml15 mr15 text-center"><%- errors.message %></p>
         <% } %>  
         <% if(typeof errors !== 'undefined' && typeof errors.msg !== 'undefined') { %>
            <p class = "alert alert-success ml15 mr15 text-center"><%- errors.msg %></p>
         <% } %>
      </div>
        <!-- End Page Title Header Ends-->
      <div class="midContentRightBar addSessionBlock">
      <div class="meddleContentSec agendaContentInfo">
        <div class="eventInfoBox">
            <div class="infoBoxContent">
                <div class="boxContentRow">
                    <span class="infoTitle">Agenda Title</span>
                    <div class="infoBoxData">
                        <input type="text" class="form-control" placeholder="Enter agenda title" name="title" id="title" value="<%=data['title'] %>">
                        <% if(typeof errors !== 'undefined' && typeof errors.title !== 'undefined') { %>
                          <p class = "error"><%- errors["title"] %></p>
                      <% } %> 
                    </div>
                </div>
         
                <div class="boxContentRow">
                    <span class="infoTitle">Agenda Date</span>
                    <div class="infoBoxData">
                        <input 
                          type="date" 
                          class="form-control" 
                          placeholder="Select agenda date"  
                          name="agenda_date" 
                          id="agenda_date" 
                          data-date="<%=data.agenda_date %>"
                          value="<%=moment(data.agenda_date*1000).format('YYYY-MM-DD') %>"
                        >
                        <% if(typeof errors !== 'undefined' && typeof errors.agenda_date !== 'undefined') { %>
                          <p class = "error"><%- errors["agenda_date"] %></p>
                      <% } %> 
                       
                    </div>
                </div>
                <div class="boxContentRow">
                  <span class="infoTitle">Start Time</span>
                  <div class="infoBoxData">
                      <input type="time" class="form-control" placeholder="Select start time" name="start_time" id="start_time" value="<%=data.start_time%>">
                      <% if(typeof errors !== 'undefined' && typeof errors.start_time !== 'undefined') { %>
                        <p class = "error"><%- errors["start_time"] %></p>
                    <% } %> 
                  </div>
              </div>
                <div class="boxContentRow">
                    <span class="infoTitle">End Time</span>
                    <div class="infoBoxData">
                        <input type="time" class="form-control" placeholder="Select end time" name="end_time" id="end_time" value="<%=data['end_time'] %>">
                        <% if(typeof errors !== 'undefined' && typeof errors.end_time !== 'undefined') { %>
                          <p class = "error"><%- errors["end_time"] %></p>
                      <% } %> 
                    </div>
                </div>
                <div class="boxContentRow">
                  <span class="infoTitle">Google Meet Url</span>
                  <div class="infoBoxData">
                      <input type="text" class="form-control" placeholder="Enter url" name="google_meet" id="google_meet" value="<%=data['google_meet'] %>">
                      <% if(typeof errors !== 'undefined' && typeof errors.google_meet !== 'undefined') { %>
                        <p class = "error"><%- errors["google_meet"] %></p>
                    <% } %> 
                  </div>
              </div>
              <div class="boxContentRow">
                <span class="infoTitle">Agenda points</span>
                <div class="infoBoxData">
                    <input type="number" class="form-control" placeholder="Enter Agenda points" name="points" id="points" value="<%=data['points'] %>">
                    <% if(typeof errors !== 'undefined' && typeof errors.points !== 'undefined') { %>
                      <p class = "error"><%- errors["points"] %></p>
                  <% } %> 
                </div>
            </div>
              
                <div class="boxContentRow">
                    <span class="infoTitle">Tags</span>
                    <div class="infoBoxData">
                        <input type="text" class="form-control tags" placeholder="Enter Tags" name="tags" id="form-tags-1" value="<%=data['tags'] %>">
                      
                        <% if(typeof errors !== 'undefined' && typeof errors.tags !== 'undefined') { %>
                          <p class = "error"><%- errors["tags"] %></p>
                      <% } %> 
                    </div>
                </div>
            </div>
        </div>
      </div>
        <!-- Start Session Description section -->
            <div class="eventInfoBox">
                <div class="infoBoxTitle">
                    <h3>Agenda Description</h3>
                </div>

                <div class="infoBoxContent">
                    <div class="boxContentTextArea">
                        <div class="infoBoxData">
                            <textarea class="form-control" rows="7" placeholder="Enter agenda description" name="description" id="description" ><%=data.description %></textarea>
                            <% if(typeof errors !== 'undefined' && typeof errors.description !== 'undefined') { %>
                              <p class = "error"><%- errors["description"] %></p>
                          <% } %> 
                        </div>
                    </div>
                </div>
            </div>
        </div>
      <!-- Submit Button -->
      <div class="adminSubmitBtn">
          <button type="submit" class="btn btn-primary adminPageBtn">Update Agenda</button>
      </div>
      <!-- Submit Button -->
  </div>
</form>
<script>
  ClassicEditor
      .create( document.querySelector( '#description' ) )
      .catch( error => {
          console.error( error );
      } );
</script>
<script src="/assets/js/jquery-3.5.1.min.js"></script>   
<script>


  $(function() {
  

    var temp1="<%=data.agenda_type %>"; 
    $("#agenda_type").val(temp1);
    var temp2="<%=data.agenda_category %>"; 
    $("#agenda_category").val(temp2);
    var temp3="<%=data.lounge_type %>"; 
    $("#lounge_type").val(temp3);

    var temp5="<%=data.featured %>"; 
    $("#featured").val(temp5);
  });
  </script>