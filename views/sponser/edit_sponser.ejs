

    <form class="form_custom"  action="/update-sponser/<%=data._id %>/<%=event_id%>" method="post">
      <div class="adiminBodyContent">
        <!-- Start Page Title Header Starts-->
          <div class="headerTitle">
              <h2>Edit Partner</h2>    
              <% if(typeof errors !== 'undefined' && typeof errors.message !== 'undefined') { %>
                <p class = "alert alert-danger ml15 mr15 text-center"><%- errors.message %></p>
             <% } %>  
             <% if(typeof errors !== 'undefined' && typeof errors.msg !== 'undefined') { %>
                <p class = "alert alert-success ml15 mr15 text-center"><%- errors.msg %></p>
             <% } %>      
          </div>
            <!-- End Page Title Header Ends-->
          <div class="midContentRightBar">
          <div class="meddleContentSec agendaContentInfo">
            <div class="eventInfoBox">
                <div class="infoBoxContent">
                  
                    <div class="boxContentRow">
                      <span class="infoTitle">Select Booth</span>
                      <div class="infoBoxData infoSelectBox">
                          <div class="form-group">
                            <select  name="booth" id="booth" class="form-control" dir="rtl">
                              <option value="">Select Booth</option>
                              <% company_data.map((offer,i) => { %>
                                <option  value="<%= offer._id %>"><%= offer.name %></option>
                                <% }) %>
                            </select>
                            <% if(typeof errors !== 'undefined' && typeof errors.booth !== 'undefined') { %>
                              <p class = "error"><%- errors["booth"] %></p>
                          <% } %> 
                          </div>
                      </div>
                  </div>
    
                    <div class="boxContentRow">
                        <span class="infoTitle">Sponsor Name</span>
                        <div class="infoBoxData">
                            <input type="text" class="form-control" name="sponsor_name" value="<%=data.sponsor_name %>" id="sponsor_name" placeholder="Enter sponsor name">
                            <% if(typeof errors !== 'undefined' && typeof errors.sponsor_name !== 'undefined') { %>
                              <p class = "error"><%- errors["sponsor_name"] %></p>
                          <% } %> 
                        </div>
                    </div>
                    
                    <div class="boxContentRow">
                      <span class="infoTitle">HyperLink</span>
                      <div class="infoBoxData">
                          <input type="text" class="form-control"  placeholder="Enter hyperlink Link" name="hyperlink1" value="<%=data.hyperlink1 %>" id="sponsor_link">
                          <% if(typeof errors !== 'undefined' && typeof errors.hyperlink1 !== 'undefined') { %>
                            <p class = "error"><%- errors["hyperlink1"] %></p>
                        <% } %> 
                      </div>
                    </div>
                   
                </div>
            </div>
            
          </div>
            
              <div class="addEventImgSec">
                <div class="infoBoxTitle">
                    <h3>Update Partner Logo (PNG file type only)</h3>
                </div>
                  <div class="addEventImg">
                      <div class="addEvntImgColl addEvntImg">
                        <div class="addEImg">
                            <input type="file" id="myFile" class="upload-img" name="image"  accept="image/png"> 
    
                            <input type="hidden" class="image_upload" name="image_data" value="<%=data.profile_img %>"/>
                            <div class="addNewEvntImg">
                                <h4>Partner Logo</h4>
                                <img src="/assets/images/navIcon/addImg.png" />
                            </div>
                           
                        </div>
                      </div>
                      <div class="addEvntImgColl agendaImageUpdated">
                        <div class="updateEventImg">
                            <img class="display_img" src="/uploads/<%=data.profile_img %>" />
                            <!-- <span class="itemTrash"><img src="/assets/images/navIcon/trash.png" /></span> -->
                        </div>
                      </div>
                      <% if(typeof errors !== 'undefined' && typeof errors.image_data !== 'undefined') { %>
                        <p class = "error"><%- errors["image_data"] %></p>
                    <% } %> 
                  </div>
              </div>
    
           
      </div>
          <!-- Submit Button -->
          <div class="adminSubmitBtn">
              <button type="submit" class="btn btn-primary adminPageBtn">Update Partner</button>
          </div>
          <!-- Submit Button -->
      </div>
    </form>
     
    
    <!-- content-wrapper ends -->
    
        
    <script src="/assets/js/jquery-3.5.1.min.js"></script>   
    <script>
    
    
      $(function() {
      
    $("#lounge_type").click(function(){
    if($("#lounge_type").val() == "360_space")
    {
      $('.space_code').show();
      $("#space_code").prop('disabled', false);
    }
    else
    {
      $('.space_code').hide();
      $("#space_code").prop('disabled', true);
    
    }
    });
    
    var temp="<%=data.booth %>"; 
    $("#booth").val(temp);
      });
      </script>       