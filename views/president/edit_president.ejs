

<link rel="stylesheet" href="http://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">	
<link rel="stylesheet" href="/assets/jquery.tagsinput-revisited.css" />

<link href="/tag/jquery.flexdatalist.css" rel="stylesheet" type="text/css">
<link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/selectize.js/0.12.6/css/selectize.bootstrap3.min.css" integrity="sha256-ze/OEYGcFbPRmvCnrSeKbRTtjG4vGLHXgOqsyLFTRjg=" crossorigin="anonymous" />


<form class="form_custom" id="" method="post" action="/update-president/<%=data._id %>/<%=event_id%>">
  <div class="adiminBodyContent">
    <!-- Start Page Title Header Starts-->
      <div class="headerTitle">
          <h2>Edit President club</h2>    
          <% if(typeof errors !== 'undefined' && typeof errors.message !== 'undefined') { %>
            <p class = "alert alert-danger ml15 mr15 text-center"><%- errors.message %></p>
         <% } %>  
         <% if(typeof errors !== 'undefined' && typeof errors.msg !== 'undefined') { %>
            <p class = "alert alert-success ml15 mr15 text-center"><%- errors.msg %></p>
         <% } %>
      </div>
        <!-- End Page Title Header Ends-->
      <div class="midContentRightBar addSessionBlock">
      <div class="meddleContentSec agendaContentInfo">
        <div class="eventInfoBox">
            <div class="infoBoxContent">
         
                <div class="boxContentRow">
                    <span class="infoTitle">Title</span>
                    <div class="infoBoxData">
                        <input type="text" class="form-control" placeholder="Enter title" name="title" id="title" value="<%=data.title %>">
                        <% if(typeof errors !== 'undefined' && typeof errors.title !== 'undefined') { %>
                          <p class = "error"><%- errors["title"] %></p>
                      <% } %> 
                    </div>
                </div>

                <div class="boxContentRow">
                  <span class="infoTitle">Designation</span>
                  <div class="infoBoxData">
                      <input type="text" class="form-control" placeholder="Enter Designation" name="designation" id="designation" value="<%=data.designation %>">
                      <% if(typeof errors !== 'undefined' && typeof errors.designation !== 'undefined') { %>
                        <p class = "error"><%- errors["designation"] %></p>
                    <% } %> 
                  </div>
              </div>
              <div class="boxContentRow">
                <span class="infoTitle">Year</span>
                <div class="infoBoxData">
                    <input type="text" class="form-control" placeholder="Enter year" name="year" id="year" value="<%=data.year %>">
                    <% if(typeof errors !== 'undefined' && typeof errors.year !== 'undefined') { %>
                      <p class = "error"><%- errors["year"] %></p>
                  <% } %> 
                </div>
            </div>

            <div class="boxContentRow">
              <span class="infoTitle">Video url</span>
              <div class="infoBoxData">
                  <input type="text" class="form-control" placeholder="Enter video url" name="video_url" id="video_url" value="<%=data.video_url %>">
                  <% if(typeof errors !== 'undefined' && typeof errors.video_url !== 'undefined') { %>
                    <p class = "error"><%- errors["video_url"] %></p>
                <% } %> 
              </div>
          </div>
              
              
            </div>
        </div>
      
      </div>
        <!-- Start Session Description section -->
            <div class="eventInfoBox">
                <div class="infoBoxTitle">
                    <h3>Description</h3>
                </div>
                <div class="infoBoxContent">
                    <div class="boxContentTextArea">
                        <div class="infoBoxData">
                            <textarea class="form-control" rows="7" placeholder="Enter description" name="description" id="description" ><%=data.description %></textarea>
                            <% if(typeof errors !== 'undefined' && typeof errors.description !== 'undefined') { %>
                              <p class = "error"><%- errors["description"] %></p>
                          <% } %> 
                        </div>
                    </div>
                </div>
            </div>
          <!-- End Session Description section -->
          <div class="addEventImgSec">
            <div class="infoBoxTitle">
                <h3>Add Image</h3>
            </div>
              <div class="addEventImg addSpeakerImg">
                  <div class="addEvntImgColl addEvntImg">
                    <div class="addEImg">
                      <input type="file" id="myFile" class="upload-img" name="image"  accept="image/*"> 
    
                      <input type="hidden" class="image_upload" name="image_data" value="<%=data.image_data %>"/>
                      <div class="addNewEvntImg">
                          <h4>Upload image</h4>
                          <img src="/assets/images/navIcon/addImg.png" />
                      </div>
                     
                  </div>
                </div>
                <div class="addEvntImgColl">
                  <div class="updateEventImg">
                    <img class="display_img" src="/uploads/<%=data.image_data %>" />
                      <!-- <span class="itemTrash"><img src="/assets/images/navIcon/trash.png" /></span> -->
                  </div>
                </div>
                <% if(typeof errors !== 'undefined' && typeof errors.image_data !== 'undefined') { %>
                  <p class = "error"><%- errors["image_data"] %></p>
              <% } %> 
              </div>
          </div>
          <!-- Add video section -->
      <div class="addEventImgSec">
        <div class="infoBoxTitle">
          <h3>Add Video</h3>
        </div>
        <div class="addEventImg">
          <div class="addEvntImgColl addEvntImg">
            <div class="addEImg">
              <input type="file" id="myFile" name="image" class="single_video" accept="video/mp4">
              <div class="video_hidden"></div>
              <div class="addNewEvntImg">
                <h4>Add Video</h4>
                <img src="/assets/images/navIcon/addImg.png" />
              </div>
            </div>
          </div>
          <div class="video_display addEvntImgColl"><div class="addEventVideoSec"><video class="eventVideo" id="myvid" controls><source src="/uploads/<%=data.video_data %>"" type="video/mp4"></video> </div><input type="hidden" class="video_upload" name="video_data" value="<%=data.video_data%>"/></div>

          <% if(typeof errors !== 'undefined' && typeof errors.video_data !== 'undefined') { %>
          <p class="error"><%- errors["video_data"] %></p>
          <% } %>
        </div>
      </div>
     
    <!-- End Add video section -->
        </div>
      <!-- Submit Button -->
      <div class="adminSubmitBtn">
          <button type="submit" class="btn btn-primary adminPageBtn">Update President club</button>
      </div>
      <!-- Submit Button -->
  </div>
</form>
<script src="/assets/js/jquery-3.5.1.min.js"></script>   



