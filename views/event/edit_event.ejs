  $(function () {
    // var dashboard = "<%=data.dashboard %>";
    // $("#dashboard").val(dashboard);
    var download = "<%=data.download %>";
    $("#download").val(download);
    
    // var theme = "<%=data.theme %>";
    // $("#theme").val(theme);
  });
</script>