<form class="forms-sample" action="/update-event/<%=data._id %>" method="post">

  <div class="content-wrapper">
    <!-- Page Title Header Starts-->
    <div class="backIcon">
      <a href="/event-list"><img src="/assets/images/navIcon/back.png" alt="Back" /></a>
    </div>
    <div class="headerTitle">
      <h2>Edit Event</h2>
      <% if(typeof errors !== 'undefined' && typeof errors.message !== 'undefined') { %>
      <p class="alert alert-danger ml15 mr15 text-center"><%- errors.message %></p>
      <% } %>
      <% if(typeof errors !== 'undefined' && typeof errors.msg !== 'undefined') { %>
      <p class="alert alert-success ml15 mr15 text-center"><%- errors.msg %></p>
      <% } %>
    </div>
    <!-- Page Title Header Ends-->
    <div class="midContentRightBar">
      <div class="meddleContentSec">
        <div class="eventInfoBox">
          <div class="infoBoxTitle">
            <h3>Enter Event Information</h3>
          </div>
          <div class="infoBoxContent">

            <div class="boxContentRow">
              <span class="infoTitle">Event Title</span>
              <div class="infoBoxData">
                <input type="text" class="form-control" name="title" value="<%=data.title %>" id="title"
                  placeholder="Enter event title">
                <% if(typeof errors !== 'undefined' && typeof errors.title !== 'undefined') { %>
                <p class="error"><%- errors["title"] %></p>
                <% } %>
              </div>
            </div>
            <div class="boxContentRow">
              <span class="infoTitle">Site url</span>
              <div class="infoBoxData">
                <input type="text" class="form-control" name="site_url" value="<%=data.site_url %>" id="site_url"
                  placeholder="https://example.com">
                <% if(typeof errors !== 'undefined' && typeof errors.site_url !== 'undefined') { %>
                <p class="error"><%- errors["site_url"] %></p>
                <% } %>
              </div>
            </div>
            <!-- <div class="boxContentRow">
              <span class="infoTitle">Choose Theme</span>
              <div class="infoBoxData infoSelectBox">
                <div class="form-group">
                  <select class="form-control" name="theme" id="theme">
                    <option value="">Select Theme</option>
                    <option value="enkindle">Enkindle</option>
                    <option value="elevate">Elevate</option>
                  </select>
                  <% if(typeof errors !== 'undefined' && typeof errors.theme !== 'undefined') { %>
                  <p class="error"><%- errors["theme"] %></p>
                  <% } %>
                </div>
              </div>
            </div> -->
            <div class="boxContentRow">
              <span class="infoTitle">Start Date</span>
              <div class="infoBoxData">
                <input type="date" class="form-control" name="start_date"
                  value="<%=moment(data.start_date).format('YYYY-MM-DD') %>" id="start_date"
                  placeholder="Enter start date" >
                <% if(typeof errors !== 'undefined' && typeof errors.start_date !== 'undefined') { %>
                <p class="error"><%- errors["start_date"] %></p>
                <% } %>
              </div>
            </div>
            <div class="boxContentRow">
              <span class="infoTitle">End Date</span>
              <div class="infoBoxData">
                <input type="date" class="form-control" name="end_date"
                  value="<%=moment(data.end_date).format('YYYY-MM-DD') %>" id="end_date" placeholder="Enter end date"
                  min="<%=currentDate %>">
                <% if(typeof errors !== 'undefined' && typeof errors.end_date !== 'undefined') { %>
                <p class="error"><%- errors["end_date"] %></p>
                <% } %>
              </div>
            </div>
            <!-- <div class="boxContentRow">
              <span class="infoTitle">Choose Dashboard</span>
              <div class="infoBoxData infoSelectBox">
                <div class="form-group">
                  <select class="form-control" name="dashboard" id="dashboard">
                    <option value="">Select Dashboard</option>
                    <option value="dashboard_1">Dashboard - 1</option>
                    <option value="dashboard_2">Dashboard - 2</option>
                  </select>
                  <% if(typeof errors !== 'undefined' && typeof errors.dashboard !== 'undefined') { %>
                  <p class="error"><%- errors["dashboard"] %></p>
                  <% } %>
                </div>
              </div>
            </div> -->
            <div class="boxContentRow">
              <span class="infoTitle">Download</span>
              <div class="infoBoxData infoSelectBox">
                <div class="form-group">
                  <select class="form-control" name="download" id="download">
                    <option value="">Select videoCheck</option>
                    <option value="true">Enable</option>
                    <option value="false">Disable</option>
                  </select>
                  <% if(typeof errors !== 'undefined' && typeof errors.download !== 'undefined') { %>
                  <p class="error"><%- errors["download"] %></p>
                  <% } %>
                </div>
              </div>
            </div>
            <!-- <div class="boxContentRow">
              <span class="infoTitle">Linkedin url</span>
              <div class="infoBoxData">
                <input type="text" class="form-control" name="linkdin" value="<%=data.linkdin %>" id="linkdin"
                  placeholder="https://example.com">
                <% if(typeof errors !== 'undefined' && typeof errors.linkdin !== 'undefined') { %>
                <p class="error"><%- errors["linkdin"] %></p>
                <% } %>
              </div>
            </div> -->
            <div class="boxContentRow">
              <span class="infoTitle">Facebook url</span>
              <div class="infoBoxData">
                <input type="text" class="form-control" name="facebook" value="<%=data.facebook %>" id="facebook"
                  placeholder="https://example.com">
                <% if(typeof errors !== 'undefined' && typeof errors.facebook !== 'undefined') { %>
                <p class="error"><%- errors["facebook"] %></p>
                <% } %>
              </div>
            </div>
            <div class="boxContentRow">
              <span class="infoTitle">Twitter url</span>
              <div class="infoBoxData">
                <input type="text" class="form-control" name="twitter" value="<%=data.twitter %>" id="twitter"
                  placeholder="https://example.com">
                <% if(typeof errors !== 'undefined' && typeof errors.twitter !== 'undefined') { %>
                <p class="error"><%- errors["twitter"] %></p>
                <% } %>
              </div>
            </div>
            <div class="boxContentRow">
              <span class="infoTitle">Instagram url</span>
              <div class="infoBoxData">
                <input type="text" class="form-control" name="instagram" value="<%=data.instagram %>" id="instagram"
                  placeholder="https://example.com">
                <% if(typeof errors !== 'undefined' && typeof errors.instagram !== 'undefined') { %>
                <p class="error"><%- errors["instagram"] %></p>
                <% } %>
              </div>
            </div>

            <div class="boxContentRow">
              <span class="infoTitle">On Demand session url</span>
              <div class="infoBoxData">
                <input type="text" class="form-control" name="demand_link" value="<%=data.demand_link %>" id="demand_link"
                  placeholder="https://example.com">
                <% if(typeof errors !== 'undefined' && typeof errors.demand_link !== 'undefined') { %>
                <p class="error"><%- errors["demand_link"] %></p>
                <% } %>
              </div>
            </div>

            <div class="boxContentRow">
              <span class="infoTitle">Rap Videos url</span>
              <div class="infoBoxData">
                <input type="text" class="form-control" name="rap_link" value="<%=data.rap_link %>" id="rap_link"
                  placeholder="https://example.com">
                <% if(typeof errors !== 'undefined' && typeof errors.rap_link !== 'undefined') { %>
                <p class="error"><%- errors["rap_link"] %></p>
                <% } %>
              </div>
            </div>
            <!-- <div class="boxContentRow">
              <span class="infoTitle">360 video</span>
              <div class="infoBoxData">
                <input type="text" class="form-control" name="video_360" value="<%=data.space_code %>" id="instagram"
                  placeholder="https://example.com">
                <% if(typeof errors !== 'undefined' && typeof errors.video_360 !== 'undefined') { %>
                <p class="error"><%- errors["video_360"] %></p>
                <% } %>
              </div>
            </div>

             -->

          </div>
        </div>

        

        <div class="eventInfoBox">
          <div class="infoBoxTitle">
            <h3>Enter Event Description</h3>
          </div>
          <div class="infoBoxContent">
            <div class="boxContentTextArea">
              <div class="infoBoxData">
                <textarea class="form-control" rows="6" name="description"
                  placeholder="Enter event description"><%=data.description %></textarea>
                <% if(typeof errors !== 'undefined' && typeof errors.description !== 'undefined') { %>
                <p class="error"><%- errors["description"] %></p>
                <% } %>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- Add Images section -->
      <div class="addEventImgSec">
        <div class="infoBoxTitle">
          <h3>Add Event Banner Images</h3>
        </div>
        <div class="addEventImg">
          <div class="addEvntImgColl addEvntImg">
            <div class="addEImg">
              <input type="file" id="myFile" name="image[]" class="multiple_images" multiple accept="image/*">
              <div class="image_hidden"></div>
              <div class="addNewEvntImg">
                <h4>Add Banner Images</h4>
                <img src="/assets/images/navIcon/addImg.png" />
              </div>
            </div>
          </div>
          <% data.images.map((offer,i) => { %>
          <div class="addEvntImgColl">
            <div class="updateEventImg">
              <img src="/uploads/<%=offer %>" />
              <span class="itemTrash"><img src="/assets/images/navIcon/trash.png" /></span>
              <input type="hidden" class="image_upload" name="image_data" value="<%=offer %>" />
            </div>
          </div>
          <% }) %>
          <div class="image_display"></div>
          <% if(typeof errors !== 'undefined' && typeof errors.image_data !== 'undefined') { %>
          <p class="error"><%- errors["image_data"] %></p>
          <% } %>

        </div>
      </div>

      <div class="eventInfoBox">
        <div class="infoBoxTitle">
          <h3>Enter Banner Hyperlinks</h3>
        </div>
        <div class="infoBoxContent">
          <div class="boxContentRow">
            <span class="infoTitle">First Banner hyperlink</span>
            <div class="infoBoxData">
              <input type="text" class="form-control" name="hyperlink1" value="<%=data.hyperlink1 %>" id="hyperlink1"
                placeholder="https://example.com">
              <% if(typeof errors !== 'undefined' && typeof errors.hyperlink1 !== 'undefined') { %>
              <p class="error"><%- errors["hyperlink1"] %></p>
              <% } %>
            </div>
          </div>

          <div class="boxContentRow">
            <span class="infoTitle">Second Banner hyperlink</span>
            <div class="infoBoxData">
              <input type="text" class="form-control" name="hyperlink2" value="<%=data.hyperlink2 %>" id="hypehyperlink2rlink1"
                placeholder="https://example.com">
              <% if(typeof errors !== 'undefined' && typeof errors.hyperlink2 !== 'undefined') { %>
              <p class="error"><%- errors["hyperlink2"] %></p>
              <% } %>
            </div>
          </div>

          <div class="boxContentRow">
            <span class="infoTitle">Third Banner hyperlink</span>
            <div class="infoBoxData">
              <input type="text" class="form-control" name="hyperlink3" value="<%=data.hyperlink3 %>" id="hyperlink3"
                placeholder="https://example.com">
              <% if(typeof errors !== 'undefined' && typeof errors.hyperlink3 !== 'undefined') { %>
              <p class="error"><%- errors["hyperlink3"] %></p>
              <% } %>
            </div>
          </div>

          <div class="boxContentRow">
            <span class="infoTitle">Fourth Banner hyperlink</span>
            <div class="infoBoxData">
              <input type="text" class="form-control" name="hyperlink4" value="<%=data.hyperlink4 %>" id="hyperlink4"
                placeholder="https://example.com">
              <% if(typeof errors !== 'undefined' && typeof errors.hyperlink4 !== 'undefined') { %>
              <p class="error"><%- errors["hyperlink4"] %></p>
              <% } %>
            </div>
          </div>
        </div>
      </div>
      <!-- End Images section -->
      <!-- Add video section -->
      <div class="addEventImgSec">
        <div class="infoBoxTitle">
          <h3>Add Event Video</h3>
        </div>
        <div class="addEventImg">
          <div class="addEvntImgColl addEvntImg">
            <div class="addEImg">
              <input type="file" id="myFile" name="image[]" class="multiple_videos" multiple accept="video/mp4">
              <div class="video_hidden"></div>
              <div class="addNewEvntImg">
                <h4>Add Video</h4>
                <img src="/assets/images/navIcon/addImg.png" />
              </div>
            </div>
          </div>
          <% data.videos.map((offer,i) => { %>
          <div class="addEvntImgColl">
            <div class="addEventVideoSec">
              <video class="eventVideo" id="myvid" controls>
                <source src="/uploads/<%=offer %>" type="video/mp4">
                Your browser does not support HTML5 video.
              </video>
              <!-- <button class="playpause" type="button" id="vidbutton<%=i%>"></button>  -->
              <span class="itemTrash"><img src="/assets/images/navIcon/trash.png" /></span>
              <input type="hidden" class="video_upload" name="video_data" value="<%=offer%>" />
            </div>
          </div>
          <% }) %>
          <div class="video_display"></div>

          <% if(typeof errors !== 'undefined' && typeof errors.video_data !== 'undefined') { %>
          <p class="error"><%- errors["video_data"] %></p>
          <% } %>
        </div>
      </div>
      <!-- End Add video section -->
    </div>
    <!-- Submit Button -->
    <div class="adminSubmitBtn">
      <button type="submit" class="btn btn-primary adminPageBtn">Update Event</button>
    </div>
    <!-- Submit Button -->
  </div>
</form>
<!-- content-wrapper ends -->

<script src="/assets/js/jquery-3.5.1.min.js"></script>

