

<link rel="stylesheet" href="http://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">	
<link rel="stylesheet" href="/assets/jquery.tagsinput-revisited.css" />

<link href="/tag/jquery.flexdatalist.css" rel="stylesheet" type="text/css">
<link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/selectize.js/0.12.6/css/selectize.bootstrap3.min.css" integrity="sha256-ze/OEYGcFbPRmvCnrSeKbRTtjG4vGLHXgOqsyLFTRjg=" crossorigin="anonymous" />

<script src="https://cdn.ckeditor.com/ckeditor5/24.0.0/classic/ckeditor.js"></script>

<form class="form_custom" id="" method="post" action="/user/update-front-user/<%=data._id %>/<%=event_id %>">
  <div class="adiminBodyContent">
    <!-- Start Page Title Header Starts-->
      <div class="headerTitle">
          <h2>Edit User</h2>    
          <% if(typeof errors !== 'undefined' && typeof errors.message !== 'undefined') { %>
            <p class = "alert alert-danger ml15 mr15 text-center"><%- errors.message %></p>
         <% } %>  
         <% if(typeof errors !== 'undefined' && typeof errors.msg !== 'undefined') { %>
            <p class = "alert alert-success ml15 mr15 text-center"><%- errors.msg %></p>
         <% } %>
      </div>
        <!-- End Page Title Header Ends-->
      <div class="midContentRightBar addSessionBlock">
      <div class="meddleContentSec agendaContentInfo">
        <div class="eventInfoBox">
            <div class="infoBoxContent">
             
                <div class="boxContentRow">
                    <span class="infoTitle">First Name</span>
                    <div class="infoBoxData">
                        <input type="text" class="form-control" placeholder="Enter first name" name="first_name" id="first_name" value="<%=data['first_name'] %>">
                        <% if(typeof errors !== 'undefined' && typeof errors.first_name !== 'undefined') { %>
                          <p class = "error"><%- errors["first_name"] %></p>
                      <% } %> 
                    </div>
                </div>

                <div class="boxContentRow">
                  <span class="infoTitle">Last Name</span>
                  <div class="infoBoxData">
                      <input type="text" class="form-control" placeholder="Enter last name" name="last_name" id="last_name" value="<%=data['last_name'] %>">
                      <% if(typeof errors !== 'undefined' && typeof errors.last_name !== 'undefined') { %>
                        <p class = "error"><%- errors["last_name"] %></p>
                    <% } %> 
                  </div>
              </div>

              <div class="boxContentRow">
                <span class="infoTitle">Email</span>
                <div class="infoBoxData">
                    <input type="email" class="form-control" placeholder="Enter email" name="email" id="email" value="<%=data['email'] %>" disabled>
                    <% if(typeof errors !== 'undefined' && typeof errors.email !== 'undefined') { %>
                      <p class = "error"><%- errors["email"] %></p>
                  <% } %> 
                </div>
            </div>
              
            
              
            </div>
        </div>
     
      </div>
     
    <!-- End Add video section -->
        </div>
      <!-- Submit Button -->
      <div class="adminSubmitBtn">
          <button type="submit" class="btn btn-primary adminPageBtn">Update User</button>
      </div>
      <!-- Submit Button -->
  </div>
</form>
<script>
  ClassicEditor
      .create( document.querySelector( '#description' ) )
      .catch( error => {
          console.error( error );
      } );
</script>
